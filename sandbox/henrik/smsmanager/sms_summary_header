<script type="text/javascript">
	function editSms(queuename,groupid, msgid)
	{
		var qObj = document.getElementById("queuename");
		var gObj = document.getElementById("grouplist");
		var mObj = document.getElementById("msglist");

		qObj.value = queuename;
		gObj.value = groupid;
		mObj.value = msgid;
		return true;
	}
</script>

